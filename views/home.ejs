    <div class="row">
      <div class="col-lg-12">
        <div class="card">
          <div class="card-header">
            <div class="cui__utils__heading mb-0">
              <strong>Recent Assignments</strong>
            </div>
            <div class="text-muted">
              Top 10 recent Assignments </div>
          </div>
          <div class="card-body">

            <form>
              <table id="operationsTable" data-toggle="table" data-pagination="true" data-search="true"
                data-buttons-align="right" data-show-refresh="true" data-buttons-class="primary" data-id-field="_id"
                data-select-item-name="_id" data-click-to-select="true" data-ajax="loadOperationsData"
                data-toolbar="#addOperationsToolbar" data-toolbar-align="right" data-filter-control="true">
                <thead>
                  <tr>
                    <!-- <th data-field="state" data-checkbox="true" data-field="_id"></th> -->
                    <th data-sortable="true" data-field="product.name" data-filter-control="input">Product Name</th>
                    <th data-sortable="true" data-field="event.name" data-filter-control="input">Event Name</th>
                    <th data-sortable="true" data-formatter="formatDateforTable" data-field="dateIn"
                      data-filter-control="input">From date</th>
                    <th data-sortable="true" data-formatter="formatDateforTable" data-field="dateOut"
                      data-filter-control="input">To date</th>
                    <th data-sortable="true" data-field="assignedTo.fullName" data-filter-control="input">Assigned
                      To
                    </th>
                    <th data-sortable="true" data-formatter="formatStatusforTable" data-search-formatter="false"
                      data-field="status" data-filter-control="select">Status</th>
                    <th data-sortable="true" data-field="description">Description</th>
                    <th data-field="operate" data-formatter="operationsEventFormatter"
                      data-events="operationsEvent">
                      Actions</th>
                  </tr>
                </thead>
              </table>
            </form>

